# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# http://editorconfig.org

root = true

[*.json]
indent_size = space
indent_size  = 2

[*.rst]
indent_style = space
indent_size  = 2
trim_trailing_whitespace = false

[*.{html,sh,less,xml,js}]
indent_style = space
indent_size  = 2

[*.php]
indent_style = space
indent_size  = 4
